            <custos>
                <default/>
            </custos>
            <canais-de-prestacao>
                <default>
                    <canal-de-prestacao id="3423" tipo="presencial">
                        <descricao>Unidades de Atendimento da Receita Federal - [Encontre a sua unidade aqui](http://idg.receita.fazenda.gov.br/contato/unidades-de-atendimento)</descricao>
                    </canal-de-prestacao>
                    <canal-de-prestacao id="3424" tipo="postal">
