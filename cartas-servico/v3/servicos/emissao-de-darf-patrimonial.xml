<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<servico xmlns="http://servicos.gov.br/v3/schema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://servicos.gov.br/v3/schema ../servico.xsd">
    <nome><![CDATA[Emissão de DARF Patrimonial]]></nome>
    <sigla/>
    <nomes-populares/>
    <descricao><![CDATA[Este serviço permite a emissão de DARF para pagamento, nos bancos autorizados, da taxa de ocupação ou foro dos imóveis da União, além de outros débitos de caráter extraordinário (multas de transferências, diferenças de laudêmios e outros). Nele serão impressos os dados de identificação do imóvel, do titular ocupante ou foreiro, a data de pagamento e os respectivos valores, a serem recolhidos ao Tesouro Nacional.]]></descricao>
    <contato/>
    <gratuito>true</gratuito>
    <solicitantes>
        <solicitante>
            <tipo>Pessoa física ou jurídica</tipo>
            <requisitos><![CDATA[Possuir débito(s) a pagar.]]></requisitos>
        </solicitante>
    </solicitantes>
    <tempo-total-estimado>
        <ate max="0" unidade="minutos"/>
        <descricao/>
    </tempo-total-estimado>
    <etapas>
        <etapa>
            <titulo>Acesse o Portal da SPU e selecione o requerimento desejado</titulo>
            <descricao><![CDATA[Acesse o Portal de Atendimento da SPU (http://e-spu.planejamento.gov.br/) e selecione o tipo de requerimento desejado.]]></descricao>
            <documentos>
                <default/>
            </documentos>
            <custos>
                <default/>
            </custos>
            <canais-de-prestacao>
                <default>
                    <canal-de-prestacao tipo="web">
                        <descricao><![CDATA[http://e-spu.planejamento.gov.br/]]></descricao>
                    </canal-de-prestacao>
                </default>
            </canais-de-prestacao>
        </etapa>
        <etapa>
            <titulo>Preencha a informação solicitada</titulo>
            <descricao><![CDATA[Informe o número do Registro Imobiliário Patrimonial - RIP do Imóvel ou o CPF/CNPJ do responsável pelos débitos.]]></descricao>
            <documentos>
                <default>
                    <item>Número do Registro Imobiliário Patrimonial - RIP do Imóvel ou o CPF/CNPJ do responsável pelos débitos.</item>
                </default>
            </documentos>
            <custos>
                <default/>
            </custos>
            <canais-de-prestacao>
                <default>
                    <canal-de-prestacao tipo="web">
                        <descricao><![CDATA[http://e-spu.planejamento.gov.br/#/conteudo/32]]></descricao>
                    </canal-de-prestacao>
                </default>
            </canais-de-prestacao>
        </etapa>
    </etapas>
    <orgao id="http://estruturaorganizacional.dados.gov.br/id/unidade-organizacional/1986"/>
    <segmentos-da-sociedade>
        <item>Cidadãos</item>
        <item>Empresas</item>
    </segmentos-da-sociedade>
    <areas-de-interesse>
        <item>Habitação</item>
    </areas-de-interesse>
    <palavras-chave>
        <item>Emissão</item>
        <item>DARF</item>
        <item>Patrimonial</item>
    </palavras-chave>
    <legislacoes>
        <item><![CDATA[Não há legislação vigente.]]></item>
    </legislacoes>
</servico>
