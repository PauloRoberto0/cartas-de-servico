<servico xmlns="http://servicos.gov.br/v3/schema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://servicos.gov.br/v3/schema ../servico.xsd">
    <nome><![CDATA[Emissão de DARF - Laudêmio]]></nome>
    <sigla/>
    <nomes-populares/>
    <descricao><![CDATA[Este serviço permite a emissão da 2ª via de DARF de laudêmio após o Cálculo de Laudêmio (1º passo para transferência de titularidade). O DARF poderá ser emitido dentro do prazo de 30 dias, contados da data de geração do cálculo de laudêmio, e deverá ser pago em um dos bancos autorizados (relação disponível na página inicial deste Portal).]]></descricao>
    <contato/>
    <gratuito>true</gratuito>
    <solicitantes>
        <solicitante>
            <tipo>Pessoa física ou jurídica</tipo>
            <requisitos><![CDATA[Que tenha realizado o cálculo de laudêmio e deseja obter a Certidão de Autorização para Transferência - CAT (2º passo para transferência de titularidade).]]></requisitos>
        </solicitante>
    </solicitantes>
    <tempo-total-estimado>
        <ate max="0" unidade="minutos"/>
        <descricao/>
    </tempo-total-estimado>
    <etapas>
        <etapa>
            <titulo>Acesse o site da SPU e preencha a informação solicitada</titulo>
            <descricao><![CDATA[Informe o número da Ficha de Cálculo de Laudêmio ou o número do Registro Imobiliário Patrimonial.]]></descricao>
            <documentos>
                <default/>
            </documentos>
            <custos>
                <default/>
            </custos>
            <canais-de-prestacao>
                <default>
                    <canal-de-prestacao tipo="web">
                        <descricao><![CDATA[http://e-spu.planejamento.gov.br/#/conteudo/16]]></descricao>
                    </canal-de-prestacao>
                </default>
            </canais-de-prestacao>
        </etapa>
    </etapas>
    <orgao id="http://estruturaorganizacional.dados.gov.br/id/unidade-organizacional/1986"/>
    <segmentos-da-sociedade>
        <item>Cidadãos</item>
        <item>Empresas</item>
        <item>Demais segmentos (ONGs, organizações sociais, etc)</item>
    </segmentos-da-sociedade>
    <areas-de-interesse>
        <item>Habitação</item>
        <item>Patrimônio</item>
    </areas-de-interesse>
    <palavras-chave>
        <item>DARF</item>
        <item>Emissão</item>
        <item>Laudêmio</item>
    </palavras-chave>
    <legislacoes>
        <item><![CDATA[Não há legislação vigente.]]></item>
    </legislacoes>
</servico>
