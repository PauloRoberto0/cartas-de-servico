                    </canal-de-prestacao>
                    <canal-de-prestacao id="4372" tipo="e-mail">
                        <descricao>O endereço informado no requerimento</descricao>
                    </canal-de-prestacao>
                </default>
            </canais-de-prestacao>
        </etapa>
