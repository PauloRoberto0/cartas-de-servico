    <id>obter-ajuda-para-terminar-o-ensino-fundamental-e-se-qualificar-profissionalmente</id>
    <dbId>1301</dbId>
    <nome>Obter ajuda para terminar o ensino fundamental e se qualificar profissionalmente</nome>
    <sigla></sigla>
    <nomes-populares>
        <item id="1730">Programa Nacional de Inclusão de Jovens</item>
        <item id="1731">Projovem</item>
        <item id="1732">Projovem Urbano</item>
        <item id="1733">PJU</item>
