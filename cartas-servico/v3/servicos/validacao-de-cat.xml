    <nome><![CDATA[Validação de CAT]]></nome>
    <sigla/>
    <nomes-populares/>
    <descricao><![CDATA[Este serviço permite verificar a autenticidade de uma Certidão de Autorização para Transferência – CAT emitida pela SPU. Para a validação devem ser utilizadas as informações impressas na CAT.]]></descricao>
    <contato/>
    <gratuito>true</gratuito>
    <solicitantes>
        <solicitante>
            <tipo>Pessoa física ou jurídica</tipo>
            <requisitos><![CDATA[Que necessite verificar a autenticidade de Certidão de Autorização para Transferência – CAT emitida pela SPU.]]></requisitos>
        </solicitante>
    </solicitantes>
    <tempo-total-estimado>
        <ate max="0" unidade="meses"/>
        <descricao><![CDATA[Não há prazo estipulado]]></descricao>
    </tempo-total-estimado>
    <etapas>
        <etapa>
            <titulo>Acesse o Portal da SPU e selecione o requerimento desejado</titulo>
            <descricao><![CDATA[Acesse o Portal de Atendimento da SPU (http://e-spu.planejamento.gov.br/) e selecione o tipo de requerimento desejado.]]></descricao>
            <documentos>
                <default/>
            </documentos>
            <custos>
                <default/>
            </custos>
            <canais-de-prestacao>
                <default>
                    <canal-de-prestacao tipo="web">
                        <descricao><![CDATA[http://e-spu.planejamento.gov.br/]]></descricao>
                    </canal-de-prestacao>
                </default>
            </canais-de-prestacao>
        </etapa>
        <etapa>
            <titulo>Preencha as informações solicitadas</titulo>
            <descricao><![CDATA[Informe os dados solicitados (Número do RIP, CPF/CNPJ, Data de Emissão, Hora de Emissão, Código de Controle, Tipo de Transação, Valor de Laudêmio Pago e Percentual de Transferência) e clique o botão OK,.]]></descricao>
            <documentos>
                <default/>
            </documentos>
            <custos>
                <default/>
            </custos>
            <canais-de-prestacao>
                <default>
                    <canal-de-prestacao tipo="web">
                        <descricao><![CDATA[http://e-spu.planejamento.gov.br/#/conteudo/18]]></descricao>
                    </canal-de-prestacao>
                </default>
            </canais-de-prestacao>
        </etapa>
