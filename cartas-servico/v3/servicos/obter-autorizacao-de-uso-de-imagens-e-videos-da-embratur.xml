                    </canal-de-prestacao>
                    <canal-de-prestacao id="1773" tipo="telefone">
                        <descricao>(61) 2023-8598 / 8624/ 8604 / 8605</descricao>
                    </canal-de-prestacao>
                </default>
            </canais-de-prestacao>
