<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<servico xmlns="http://servicos.gov.br/v3/schema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://servicos.gov.br/v3/schema ../servico.xsd">
    <nome><![CDATA[Consulta de Dominialidade]]></nome>
    <sigla/>
    <nomes-populares>
        <item>Declaração de Domínio da União</item>
    </nomes-populares>
    <descricao><![CDATA[Este serviço permite ao requerente obter Declaração de Domínio da União, com efeito positivo ou negativo, em relação ao imóvel objeto de consulta.]]></descricao>
    <contato/>
    <gratuito>true</gratuito>
    <solicitantes>
        <solicitante>
            <tipo>Cartório de Registro de Imóveis/Cartório de Registro de Títulos e Documentos</tipo>
            <requisitos><![CDATA[Que vise o reconhecimento extrajudicial de usucapião, nos termos do art. 216-A da Lei nº 6.015, de 1973]]></requisitos>
        </solicitante>
        <solicitante>
            <tipo>Pessoa física</tipo>
            <requisitos><![CDATA[Que tenha capacidade para fins de processo administrativo nos termos do art. 10 da Lei nº 9.784, de 1999 (maiores de 18 anos) e tenha qualidade de interessado (art. 9º da Lei nº 9.784, de 1999)]]></requisitos>
        </solicitante>
        <solicitante>
            <tipo>Pessoa jurídica de direito privado</tipo>
            <requisitos><![CDATA[Que tenha qualidade de interessado nos termos do art. 9º da Lei nº 9.784, de 1999]]></requisitos>
        </solicitante>
        <solicitante>
            <tipo>Advocacia-Geral da União</tipo>
            <requisitos/>
        </solicitante>
        <solicitante>
            <tipo>Órgão ou entidade pública</tipo>
            <requisitos><![CDATA[Que tenha qualidade de interessado nos termos do art. 9º da Lei nº 9.784, de 1999]]></requisitos>
        </solicitante>
    </solicitantes>
    <tempo-total-estimado>
        <ate max="6" unidade="meses"/>
        <descricao/>
    </tempo-total-estimado>
    <etapas>
        <etapa>
            <titulo>Solicitar a Declaração</titulo>
            <descricao><![CDATA[O interessado deve acessar o site da SPU e preencher as informações pedidas.]]></descricao>
            <documentos>
                <default>
                    <item>CPF/CNPJ</item>
                    <item>Dados sobre o imóvel</item>
                    <item>Fotos do imóvel</item>
                </default>
            </documentos>
            <custos>
                <default/>
            </custos>
            <canais-de-prestacao>
                <default>
                    <canal-de-prestacao tipo="web">
                        <descricao><![CDATA[http://e-spu.planejamento.gov.br/#/requerimentoConsultaDominialidade?servico=35#dados-documentos]]></descricao>
                    </canal-de-prestacao>
                </default>
            </canais-de-prestacao>
        </etapa>
        <etapa>
            <titulo>Receber a declaração</titulo>
            <descricao><![CDATA[A SPU fornecerá a declaração positiva ou negativa de dominialidade do imóvel.]]></descricao>
            <documentos>
                <default>
                    <item>CPF/CNPJ</item>
                </default>
            </documentos>
            <custos>
                <default/>
            </custos>
            <canais-de-prestacao>
                <default>
                    <canal-de-prestacao tipo="web">
                        <descricao><![CDATA[http://e-spu.planejamento.gov.br/#/requerimentoConsultaDominialidade?servico=35#dados-documentos]]></descricao>
                    </canal-de-prestacao>
                </default>
            </canais-de-prestacao>
        </etapa>
    </etapas>
    <orgao id="http://estruturaorganizacional.dados.gov.br/id/unidade-organizacional/1986"/>
    <segmentos-da-sociedade>
        <item>Cidadãos</item>
        <item>Demais segmentos (ONGs, organizações sociais, etc)</item>
        <item>Empresas</item>
        <item>Órgãos e entidades públicas</item>
    </segmentos-da-sociedade>
    <areas-de-interesse>
        <item>Habitação</item>
        <item>Patrimônio</item>
    </areas-de-interesse>
    <palavras-chave>
        <item>domínio</item>
        <item>certidão</item>
        <item>confirmação</item>
    </palavras-chave>
    <legislacoes>
        <item><![CDATA[Lei nº 6.015, de 1973
Lei nº 9.784, de 1999]]></item>
    </legislacoes>
</servico>
