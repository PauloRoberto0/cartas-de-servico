                    <canal-de-prestacao tipo="web">
                        <descricao><![CDATA[http://atendimentovirtual.spu.planejamento.gov.br/Consultas/Par_Pedido.asp]]></descricao>
                    </canal-de-prestacao>
                </default>
            </canais-de-prestacao>
        </etapa>
