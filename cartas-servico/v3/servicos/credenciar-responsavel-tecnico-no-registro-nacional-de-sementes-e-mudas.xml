                    <canal-de-prestacao id="4256" tipo="web">
                        <descricao>https://consulta.tesouro.fazenda.gov.br/gru_novosite/gru_simples.asp</descricao>
                    </canal-de-prestacao>
                </default>
            </canais-de-prestacao>
        </etapa>
        <etapa id="4864">
            <titulo>Solicitar Credenciamento</titulo>
            <descricao>O formulário e o comprovante de pagamento devem ser entregues, junto com o restante da documentação necessária, em uma SFA.</descricao>
            <documentos>
                <default>
                    <item id="4548">Formulário de requerimento preenchido e assinado</item>
                    <item id="4549">Comprovante de pagamento da GRU</item>
                    <item id="4550">Cópia do CPF</item>
                    <item id="4551">Comprovante de registro profissional no CREA</item>
                    <item id="4552">Declaração do interessado de que está adimplente junto ao MAPA</item>
                </default>
            </documentos>
