            </custos>
            <canais-de-prestacao>
                <default>
                    <canal-de-prestacao id="4038" tipo="web">
                        <descricao>http://www.patrimoniodetodos.gov.br/#/</descricao>
                    </canal-de-prestacao>
                </default>
            </canais-de-prestacao>
        </etapa>
    </etapas>
