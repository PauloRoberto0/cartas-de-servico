                    </canal-de-prestacao>
                </default>
            </canais-de-prestacao>
        </etapa>
        <etapa id="3013">
            <titulo>Receber o resultado</titulo>
            <descricao></descricao>
            <documentos>
                <default>
                    <item id="4009">Número do pedido</item>
