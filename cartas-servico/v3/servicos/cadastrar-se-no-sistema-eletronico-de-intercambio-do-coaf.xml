            <titulo>Solicitar cadastramento</titulo>
            <descricao></descricao>
            <documentos>
                <default>
                    <item id="3231">CPF</item>
                    <item id="3232">formulário próprio assinado</item>
                </default>
            </documentos>
            <custos>
                <default/>
            </custos>
            <canais-de-prestacao>
                <default>
                    <canal-de-prestacao id="3445" tipo="web">
                        <descricao>http://www.coaf.sei.fazenda.gov.br/web_pedidos/FormularioMain.asp</descricao>
                    </canal-de-prestacao>
                    <canal-de-prestacao id="3446" tipo="postal">
                        <descricao>SAUN Qd 5 Lote C  Torre D   2º andar - CEP: 70.040-250 - Asa Norte - Brasília - DF</descricao>
                    </canal-de-prestacao>
                </default>
            </canais-de-prestacao>
        </etapa>
        <etapa id="2654">
            <titulo>Receber senha</titulo>
            <descricao></descricao>
            <documentos>
