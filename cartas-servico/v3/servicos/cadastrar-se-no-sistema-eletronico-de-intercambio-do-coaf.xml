    <id>cadastrar-se-no-sistema-eletronico-de-intercambio-do-coaf</id>
    <dbId>1007</dbId>
    <nome>Cadastrar-se no Sistema Eletrônico de Intercâmbio do COAF</nome>
    <sigla>SEI-C</sigla>
    <descricao>Cadastro de autoridades competentes no Sistema Eletrônico de Intercâmbio (SEI-C), a fim de que possam solicitar e receber informações sobre inteligência financeira do COAF</descricao>
    <contato></contato>
    <gratuito>true</gratuito>
    <porcentagem-manual>false</porcentagem-manual>
    <solicitantes>
        <solicitante id="1231">
            <tipo>Autoridade competente para apuração (investigação) do crime de lavagem de dinheiro ou de qualquer outro ilícito;</tipo>
            <requisitos>Autoridade responsável pelo procedimento de investigação objeto do pedido</requisitos>
        </solicitante>
    </solicitantes>
    <tempo-total-estimado>
        <entre min="2" max="7" unidade="dias-corridos"/>
        <descricao></descricao>
    </tempo-total-estimado>
    <validade-documento>
        <descricao></descricao>
    </validade-documento>
    <etapas>
