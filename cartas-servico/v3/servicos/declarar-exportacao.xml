                    <item id="4427">Nota Fiscal</item>
                    <item id="4428">Certificados</item>
                    <item id="4429">Dados do bem e da operação de exportação</item>
                </default>
            </documentos>
            <custos>
                <default/>
            </custos>
            <canais-de-prestacao>
